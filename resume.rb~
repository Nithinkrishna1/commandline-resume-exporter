require_relative 'available_formats'
class Resume
  def user_details
    content = ["name","age","total_cgpa"]
    values =[]
    content.each do |item|
      puts "enter the #{item}"
      values.push(gets.chomp) 
    end
  end
  include Formats
  def checks_export_availability
    puts available_formats
  end
end

res1= Resume.new
res1.user_details
res1.checks_export_availability
